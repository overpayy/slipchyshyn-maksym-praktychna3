#include <iostream>
using namespace std;

int gcd(int a, int b) {
    while (b != 0) {
        int r = a % b;
        a = b;
        b = r;
    }
    return a;
}

int lcm(int a, int b) {
    return (a * b) / gcd(a, b);
}

int main() {
    int x, y;
    cout << "Enter two integers: ";
    cin >> x >> y;

    int result = lcm(x, y);

    cout << "The least common multiple of " << x << " and " << y << " is " << result << endl;

    return 0;
}
